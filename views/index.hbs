<main class="main-list">
  {{#each list}}
  <section class="list-item">
    <h2 class="list-title">{{titulo}}</h2>
    <div class="list-description">
      <div class="list-photo">
        <img src="{{ foto }}" alt="">
      </div>
      <div class="list-text">
        <p><span>Descripción:</span></p>
        <p>{{descripcion}}</p><hr>
        <p><span>Localización:</span></p>
        <p>{{localizacion}}</p><hr>
        <p><span>Fecha:</span></p>
        <p>{{fecha}}</p>
      </div>
    </div>
    <div class="list-container">
      <p class="info-button">Más información</p>
      <div class="list-overlay">
        <p class="name-button">Nombre: {{nombre}} / Teléfono: {{telefono}}</p>
      </div>
    </div>
  </section>
  {{/each}}
</main>